<% provide :title do %>
  <%= t("admin.header.title") %> - <%= t("admin.menu.legislation") %> - <%= @process.title %> - <%= t("admin.legislation.topics.document.title") %>
<% end %>

<div class="legislation-admin legislation-draft-versions-index">

  <%= back_link_to admin_legislation_process_topics_path, t("shared.back") %>

  <h2><%= @process.title %></h2>

  <%= render "admin/legislation/processes/subnav", process: @process, active: "topics" %>

  <%= render "topic_levels" %>

  <% if @topic_levels.any? %>
    <%= render "topics" %>
  <% end %>

</div>
