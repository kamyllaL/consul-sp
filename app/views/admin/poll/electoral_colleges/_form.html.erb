<%= form_for(@electoral_college, url: form_url) do |f| %>

  <%= render "shared/errors", resource: @electoral_college %>

  <div class="row">
    <div class="small-12">
      <%= f.hidden_field :poll_id, value: @poll.id %>
      <%= f.text_field :title %>
    </div>
  </div>

  <div class="row">
    <div class="small-12 medium-4 large-2 margin-top column">
      <%= f.submit(class: "button success expanded", value: t("shared.save")) %>
    </div>
  </div>

<% end %>
