<% if documents.any? %>
  <div id="documents" class="documents">
    <h2><%= t("documents.title") %>&nbsp;<span>(<%= documents.count %>)</span></h2>

    <ul class="no-bullet document-link">
      <%= render partial: "documents/document", collection: documents, not_destroy: not_destroy %>
    </ul>
  </div>
<% end %>
