<div class="row">
  <div class="small-12 column">
    <% if feasibility_analyzable.feasibility_analyses.empty? ||
      !feasibility_analyzable.should_show_feasibility_analysis? %>
      <div class="callout primary text-center margin-top">
        <%= t("feasibility_analyses.index.no_feasibility_analyses") %>
      </div>
    <% else %>
      <section>
        <ul class="no-bullet">
          <% feasibility_analyzable.feasibility_analyses.each do |feasibility_analysis| %>
            <%= render "feasibility_analyses/feasibility_analysis", feasibility_analysis: feasibility_analysis %>
          <% end %>
        </ul>
      </section>
    <% end %>
  </div>
</div>
